<?xml version="1.0"?>

<launch>
    <rosparam command="load" file="$(find putm_dv_vision)/config/config.yaml" />

    <group ns="vision_node">
        <node name="vision" pkg="putm_dv_vision" type="vision.py" output="screen" />
    </group>

    <node type="rviz" name="rviz" pkg="rviz" args="-d $(find putm_dv_vision)/rviz/rviz_config.rviz" />
</launch>